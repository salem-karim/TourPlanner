<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.text.Font?>
<ButtonBar xmlns="http://javafx.com/javafx"
           xmlns:fx="http://javafx.com/fxml"
           fx:id="buttonBar"
           buttonMinWidth="80.0" prefHeight="40.0" prefWidth="200.0"
           fx:controller="at.technikum.frontend.PL.controllers.OKCancelButtonBarController">
  <buttons>
    <Button fx:id="okButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
            text="%button.ok" textAlignment="CENTER">
      <font>
        <Font size="15.0"/>
      </font>
    </Button>
    <Button fx:id="cancelButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
            text="%button.cancel" textAlignment="CENTER">
      <font>
        <Font size="15.0"/>
      </font>
    </Button>
  </buttons>
  <opaqueInsets>
    <Insets/>
  </opaqueInsets>
  <padding>
    <Insets bottom="10.0" right="20.0"/>
  </padding>
</ButtonBar>
